<RIBCL VERSION="2.0">
        <LOGIN USER_LOGIN="<%= @username %>" PASSWORD="<%= @password %>">
                <USER_INFO MODE="write">
                        <MOD_USER USER_LOGIN="<%= @username %>">
                                <PASSWORD value="<%= @password %>"/>
                        </MOD_USER>
                </USER_INFO>
        </LOGIN>
</RIBCL>
